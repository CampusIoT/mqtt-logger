{
  "id": "mqtt-logger-1",
  "name": "MQTT Logger #1",

  "env": "CampusIoT",

  "loglevel": {
    "error" : true,
    "info" : false,
    "debug" : false
  },

  "mqtt": [
     {
      "name" : "MQTT TEST MOSQUITTO",
      "mqttClientUrl": "mqtt://test.mosquitto.org",
      "mqttClientIdPrefix": "campusiot",
      "mqttTopic": "campusiot/#",
      "mqttOptions": {
        "keepalive": 60,
        "reconnectPeriod": 10000
      }
    }
  ]
}
